<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.Theme.AppCompat" parent="@style/Base.V26.Theme.AppCompat">
    </style>
    <style name="Base.Theme.AppCompat.Light" parent="@style/Base.V26.Theme.AppCompat.Light">
    </style>
    <style name="Base.V26.Theme.AppCompat" parent="@style/Base.V23.Theme.AppCompat">
        <item name="colorError">?android:attr/colorError</item>
    </style>
    <style name="Base.V26.Theme.AppCompat.Light" parent="@style/Base.V23.Theme.AppCompat.Light">
        <item name="colorError">?android:attr/colorError</item>
    </style>
    <style name="Base.V26.Widget.AppCompat.Toolbar" parent="@style/Base.V7.Widget.AppCompat.Toolbar">
        <item name="android:touchscreenBlocksFocus">true</item>
        <item name="android:keyboardNavigationCluster">true</item>
    </style>
    <style name="Base.Widget.AppCompat.Toolbar" parent="@style/Base.V26.Widget.AppCompat.Toolbar">
    </style>
    <style name="BetaTagTextAppearance" parent="@style/TextAppearance.GoogleMaterial.Caption">
        <item name="android:textColor">@color/mod_daynight_grey800</item>
        <item name="android:gravity">0x11</item>
        <item name="android:background">@drawable/beta_tag_background</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:layout_marginStart">@dimen/beta_tag_padding</item>
        <item name="android:paddingHorizontal">@dimen/beta_tag_padding</item>
    </style>
    <style name="EventsUgcErrorTextAppearance" parent="@style/TextAppearance.GoogleMaterial.Caption">
        <item name="android:textColor">?android:attr/colorError</item>
    </style>
    <style name="EventsUgcLocationPickerErrorTextAppearance" parent="@style/TextAppearance.GoogleMaterial.Body2">
        <item name="android:textColor">?android:attr/colorError</item>
    </style>
    <style name="PhotoPostComment">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/mod_daynight_grey800</item>
        <item name="android:textColorHint">@color/mod_daynight_grey650</item>
        <item name="android:gravity">0x50</item>
        <item name="android:background">@drawable/comment_outline</item>
        <item name="android:paddingTop">@dimen/photo_comment_padding_vertical</item>
        <item name="android:paddingBottom">@dimen/photo_comment_padding_vertical</item>
        <item name="android:scrollbars">0x200</item>
        <item name="android:layout_marginTop">@dimen/photo_comment_margin_top</item>
        <item name="android:minHeight">@dimen/min_touch_size</item>
        <item name="android:maxLines">4</item>
        <item name="android:selectAllOnFocus">false</item>
        <item name="android:lineSpacingExtra">5sp</item>
        <item name="android:inputType">0x64001</item>
        <item name="android:paddingStart">@dimen/photo_comment_padding_horizontal</item>
        <item name="android:paddingEnd">@dimen/photo_comment_padding_horizontal</item>
        <item name="android:layout_marginHorizontal">@dimen/photo_comment_margin_horizontal</item>
    </style>
    <style name="SudBaseThemeGlif.Light" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:colorBackground">@color/sud_glif_background_color_light</item>
        <item name="android:listPreferredItemHeight">@dimen/sud_items_preferred_height</item>
        <item name="android:textColorLink">@color/sud_color_accent_glif_light</item>
        <item name="android:windowAnimationStyle">@style/Animation.SudWindowAnimation</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowSoftInputMode">0x10</item>
        <item name="android:textAppearanceListItemSmall">?attr/textAppearanceListItemSmall</item>
        <item name="android:listPreferredItemPaddingStart">?attr/sudMarginSides</item>
        <item name="android:listPreferredItemPaddingEnd">?attr/sudMarginSides</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/black</item>
        <item name="android:indeterminateTint">?attr/colorControlActivated</item>
        <item name="android:indeterminateTintMode">src_in</item>
        <item name="android:colorError">@color/sud_color_error_text_light</item>
        <item name="colorAccent">@color/sud_color_accent_glif_light</item>
        <item name="colorPrimary">?attr/colorAccent</item>
        <item name="listPreferredItemPaddingLeft">?attr/sudMarginSides</item>
        <item name="listPreferredItemPaddingRight">?attr/sudMarginSides</item>
        <item name="sucFooterBarButtonAllCaps">?attr/sudButtonAllCaps</item>
        <item name="sucFooterBarButtonColorControlHighlightRipple">?attr/colorAccent</item>
        <item name="sucFooterBarButtonCornerRadius">?attr/sudButtonCornerRadius</item>
        <item name="sucFooterBarButtonFontFamily">?attr/sudButtonFontFamily</item>
        <item name="sucFooterBarButtonHighlightAlpha">?attr/sudButtonHighlightAlpha</item>
        <item name="sucFooterBarPaddingVertical">@dimen/sud_glif_footer_padding_vertical</item>
        <item name="sucStatusBarBackground">?android:attr/colorBackground</item>
        <item name="sucSystemNavBarBackgroundColor">@android:color/black</item>
        <item name="sudButtonAllCaps">true</item>
        <item name="sudButtonCornerRadius">@dimen/sud_glif_button_corner_radius</item>
        <item name="sudButtonFontFamily">sans-serif-medium</item>
        <item name="sudButtonHighlightAlpha">0.12</item>
        <item name="sudColorPrimary">?attr/colorPrimary</item>
        <item name="sudDividerInsetEnd">0dp</item>
        <item name="sudDividerInsetStart">@dimen/sud_items_glif_icon_divider_inset</item>
        <item name="sudDividerInsetStartNoIcon">@dimen/sud_items_glif_text_divider_inset</item>
        <item name="sudFillContentLayoutStyle">@style/SudFillContentLayout</item>
        <item name="sudGlifHeaderGravity">0x800003</item>
        <item name="sudGlifIconStyle">@style/SudGlifIcon</item>
        <item name="sudItemDescriptionStyle">@style/SudItemContainer.Description.Glif</item>
        <item name="sudItemDescriptionTitleStyle">@style/SudItemTitle.GlifDescription</item>
        <item name="sudListItemIconColor">@color/sud_list_item_icon_color_light</item>
        <item name="sudMarginSides">@dimen/sud_glif_margin_sides</item>
        <item name="sudScrollIndicators">0x2</item>
        <item name="textAppearanceListItem">@style/TextAppearance.SudGlifItemTitle</item>
        <item name="textAppearanceListItemSmall">@style/TextAppearance.SudGlifItemSummary</item>
    </style>
    <style name="Widget.Design.AppBarLayout" parent="@android:style/Widget">
        <item name="android:background">?attr/colorPrimary</item>
        <item name="android:stateListAnimator">@animator/design_appbar_state_list_animator</item>
        <item name="android:touchscreenBlocksFocus">true</item>
        <item name="android:keyboardNavigationCluster">true</item>
    </style>
</resources>
