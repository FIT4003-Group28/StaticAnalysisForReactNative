<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="4dp">
        <shape android:shape="rectangle">
            <solid android:color="?attr/ytOverlayBackgroundMedium"/>
            <corners android:topLeftRadius="4dp" android:bottomLeftRadius="4dp"/>
        </shape>
    </item>
    <item android:gravity="right">
        <shape android:shape="rectangle">
            <size android:width="4dp"/>
            <corners android:topRightRadius="4dp" android:bottomRightRadius="4dp"/>
            <solid android:color="?attr/ytOverlayIconActiveCallToAction"/>
        </shape>
    </item>
</layer-list>
