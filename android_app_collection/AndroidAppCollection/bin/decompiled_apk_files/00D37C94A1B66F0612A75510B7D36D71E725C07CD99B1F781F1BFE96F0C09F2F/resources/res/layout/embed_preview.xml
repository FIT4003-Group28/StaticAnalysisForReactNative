<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/embed_preview" android:background="@color/yt_black_pure" android:layout_width="match_parent" android:layout_height="match_parent">
    <ImageView android:id="@+id/embed_preview_thumbnail" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="centerCrop"/>
    <View android:id="@+id/top_bar_background" android:background="@drawable/player_top_bar" android:layout_width="match_parent" android:layout_height="56dp" android:layout_alignParentTop="true"/>
    <com.google.android.apps.youtube.embeddedplayer.service.ui.watchlaterbutton.remoteloaded.WatchLaterButton android:id="@+id/embed_preview_watch_later_button" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_marginTop="18dp" android:layout_marginRight="5dp" android:layout_alignParentTop="true" android:layout_alignWithParentIfMissing="true" android:layout_toStartOf="@+id/embed_preview_share_button"/>
    <com.google.android.libraries.youtube.common.ui.YouTubeButton android:textAppearance="@style/ShareButtonLabel" android:id="@+id/embed_preview_share_button" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_marginTop="18dp" android:layout_marginRight="10dp" android:text="@string/share" android:drawableTop="@drawable/share_button_icon" android:layout_alignParentTop="true" android:contentDescription="@string/accessibility_share" android:layout_alignParentEnd="true" style="@style/PlayerButton"/>
    <TextView android:id="@+id/embed_title" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="17dp" android:layout_alignParentTop="true" android:layout_marginStart="12dp" android:layout_marginEnd="4dp" android:layout_toStartOf="@+id/embed_preview_watch_later_button" android:layout_alignParentStart="true" style="@style/EmbedVideoTitle"/>
