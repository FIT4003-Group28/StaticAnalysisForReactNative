<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_gravity="left" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
        <FrameLayout android:id="@+id/page_up_container" android:focusable="false" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1" android:hapticFeedbackEnabled="false">
            <ImageView android:layout_gravity="top|center_horizontal" android:id="@+id/page_up" android:background="@drawable/gearhead_sdk_pagination_background" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="0x7f0801b6" android:scaleType="center"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/page_down_container" android:focusable="false" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1" android:hapticFeedbackEnabled="false">
            <ImageView android:layout_gravity="bottom|center_horizontal" android:id="@+id/page_down" android:background="@drawable/gearhead_sdk_pagination_background" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="0x7f0801b4" android:scaleType="center"/>
        </FrameLayout>
    </LinearLayout>
    <FrameLayout android:id="@+id/filler" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="@dimen/gearhead_sdk_paged_list_view_scrollbar_thumb_margin" android:layout_marginBottom="@dimen/gearhead_sdk_paged_list_view_scrollbar_thumb_margin">
        <ImageView android:layout_gravity="center_horizontal" android:id="@+id/scrollbar_thumb" android:background="@color/gearhead_sdk_scrollbar_thumb" android:layout_width="6dp" android:layout_height="0dp"/>
    </FrameLayout>
</FrameLayout>
