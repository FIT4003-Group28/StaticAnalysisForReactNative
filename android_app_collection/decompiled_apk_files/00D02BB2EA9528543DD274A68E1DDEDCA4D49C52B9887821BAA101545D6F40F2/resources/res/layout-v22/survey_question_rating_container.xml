<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:orientation="horizontal" android:id="@+id/survey_rating_images_container" android:paddingLeft="6dp" android:paddingRight="6dp" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <LinearLayout android:orientation="horizontal" android:paddingLeft="6dp" android:paddingRight="6dp" android:paddingBottom="16dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <FrameLayout android:paddingLeft="10dp" android:paddingRight="10dp" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.5">
            <TextView android:textSize="12sp" android:textColor="@color/survey_primary_text_color" android:layout_gravity="start" android:id="@+id/survey_rating_low_value_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:importantForAccessibility="no" android:autoSizeTextType="uniform"/>
        </FrameLayout>
        <FrameLayout android:paddingLeft="10dp" android:paddingRight="10dp" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="0.5">
            <TextView android:textSize="12sp" android:textColor="@color/survey_primary_text_color" android:layout_gravity="end" android:id="@+id/survey_rating_high_value_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:importantForAccessibility="no" android:autoSizeTextType="uniform"/>
        </FrameLayout>
    </LinearLayout>
</merge>
