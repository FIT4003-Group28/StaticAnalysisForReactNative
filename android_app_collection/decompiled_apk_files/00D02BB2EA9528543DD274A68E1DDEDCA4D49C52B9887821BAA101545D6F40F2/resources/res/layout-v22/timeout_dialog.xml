<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_gravity="center" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingStart="40dp" android:paddingEnd="40dp">
    <com.google.android.apps.gmm.base.views.webimageview.WebImageView android:id="@+id/timeout_image" android:paddingLeft="54dp" android:paddingTop="37dp" android:paddingRight="54dp" android:paddingBottom="38dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingHorizontal="54dp"/>
    <TextView android:gravity="center" android:id="@+id/timeout_title" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="6dp" android:text="@string/ARWN_TIMEOUT_DIALOG_TITLE" style="?attr/arDialogTitleStyle"/>
