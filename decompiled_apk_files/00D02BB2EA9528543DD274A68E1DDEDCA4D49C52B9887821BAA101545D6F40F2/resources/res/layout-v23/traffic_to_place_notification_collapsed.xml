<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/traffic_to_place_notification" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:id="@+id/traffic_to_place_notification_basic_thumbnail_text_layout" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_toStartOf="@+id/traffic_to_place_notification_mini_map_collapsed" android:layout_alignParentStart="true">
        <TextView android:id="@+id/traffic_to_place_notification_title" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/TrafficToPlaceNotificationCollapsedTitle"/>
        <TextView android:id="@+id/traffic_to_place_notification_subtitle" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/TrafficToPlaceNotificationCollapsedSubtitle"/>
    </LinearLayout>
    <ImageView android:id="@+id/traffic_to_place_notification_mini_map_collapsed" android:layout_width="wrap_content" android:layout_height="match_parent" android:contentDescription="@null" android:layout_alignParentEnd="true" style="@style/TrafficToPlaceNotificationMinimapCollapsed"/>
</RelativeLayout>
