<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:card_view="http://schemas.android.com/apk/res-auto">
    <androidx.cardview.widget.CardView android:id="@+id/overlay_card_view" android:background="@color/google_white" android:layout_width="match_parent" android:layout_height="wrap_content" android:contentDescription="@null" card_view:cardBackgroundColor="@color/google_white">
        <LinearLayout android:orientation="vertical" android:background="@color/google_white" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="@dimen/overlay_margin" android:contentDescription="@null">
            <LinearLayout android:id="@+id/overlay_header" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/overlay_header_bottom_margin" android:contentDescription="@null">
                <ImageView android:layout_gravity="start|center_vertical" android:id="@+id/display_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:contentDescription="@null"/>
                <TextView android:textSize="14sp" android:textColor="@color/google_grey800" android:layout_gravity="center_vertical" android:id="@+id/title" android:layout_width="0dp" android:layout_height="20dp" android:layout_marginLeft="16.5dp" android:layout_weight="1" android:fontFamily="@font/google_sans_medium" android:layout_marginStart="16.5dp"/>
