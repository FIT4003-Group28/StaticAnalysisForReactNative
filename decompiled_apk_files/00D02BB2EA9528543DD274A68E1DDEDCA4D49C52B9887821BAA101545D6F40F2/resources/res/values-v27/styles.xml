<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.Theme.GoogleMaterial.Light" parent="@style/Base.V27.Theme.GoogleMaterial.Light">
    </style>
    <style name="Base.Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@style/Base.V27.Theme.GoogleMaterial.Light.BottomSheetDialog">
    </style>
    <style name="Base.V27.Theme.GoogleMaterial.Light" parent="@style/Base.V24.Theme.GoogleMaterial.Light">
        <item name="android:navigationBarColor">@color/google_white</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    <style name="Base.V27.Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@style/Base.V21.Theme.GoogleMaterial.Light.BottomSheetDialog">
        <item name="android:navigationBarColor">@color/google_white</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">@android:color/transparent</item>
    </style>
    <style name="GmmBaseTheme" parent="@style/Theme.GoogleMaterial.Light.NoActionBar.Bridge">
        <item name="android:textColorHint">@color/no_type_grey</item>
        <item name="android:alertDialogTheme">@style/AlertDialogAccessibleTheme</item>
        <item name="android:editTextBackground">@drawable/abc_edit_text_material</item>
        <item name="android:colorAccent">@color/quantum_googblue</item>
        <item name="android:windowDrawsSystemBarBackgrounds">false</item>
        <item name="android:navigationBarDividerColor">@color/divider_grey</item>
        <item name="android:forceDarkAllowed">false</item>
        <item name="colorAccent">@color/quantum_googblue</item>
        <item name="editTextBackground">@drawable/abc_edit_text_material</item>
        <item name="preferenceFragmentListStyle">@style/PreferenceFragmentList.Material</item>
        <item name="preferenceTheme">@style/GmmPreferenceTheme</item>
        <item name="snackbarButtonStyle">@style/Widget.GoogleMaterial.Button.TextButton.Snackbar</item>
        <item name="snackbarStyle">@style/Widget.GoogleMaterial.Snackbar</item>
        <item name="snackbarTextViewStyle">@style/Widget.GoogleMaterial.Snackbar.TextView</item>
    </style>
    <style name="GmmDayNightTheme" parent="@style/Theme.GoogleMaterial.DayNight.NoActionBar.Bridge">
        <item name="android:textColorHint">@color/no_type_daynight_grey</item>
        <item name="android:alertDialogTheme">@style/AlertDialogAccessibleTheme</item>
        <item name="android:editTextBackground">@drawable/abc_edit_text_material</item>
        <item name="android:colorAccent">@color/qu_daynight_google_blue_500</item>
        <item name="android:windowDrawsSystemBarBackgrounds">false</item>
        <item name="android:timePickerDialogTheme">@style/TimePickerDialogTheme</item>
        <item name="android:datePickerDialogTheme">@style/DialogAccessibleTheme</item>
        <item name="android:navigationBarDividerColor">@color/divider_daynight_grey</item>
        <item name="android:forceDarkAllowed">false</item>
        <item name="alertDialogTheme">@style/AlertDialogAccessibleTheme</item>
        <item name="colorAccent">@color/qu_daynight_google_blue_500</item>
        <item name="editTextBackground">@drawable/abc_edit_text_material</item>
        <item name="preferenceFragmentListStyle">@style/PreferenceFragmentList.Material</item>
        <item name="preferenceTheme">@style/GmmPreferenceTheme</item>
        <item name="snackbarButtonStyle">@style/Widget.GoogleMaterial.Button.TextButton.Snackbar</item>
        <item name="snackbarStyle">@style/Widget.GoogleMaterial.Snackbar</item>
        <item name="snackbarTextViewStyle">@style/Widget.GoogleMaterial.Snackbar.TextView</item>
    </style>
    <style name="OneGoogle.Popover" parent="@style/OneGoogle.PopoverBase">
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowTranslucentNavigation">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
</resources>
